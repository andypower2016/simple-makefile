CC = gcc
CFLAGS = 
INC = -I"./module"
LIB_PATH = -L"./module"

.PHONY: main
main:
	$(CC) -c ./module/module.c -o ./module/module.o
	$(CC) -fPIC -shared -o ./module/libmodule.so ./module/module.o # add lib in prefix of module, can use -l when linking
	$(CC) main.c $(INC) $(LIB_PATH) -lmodule -o main
